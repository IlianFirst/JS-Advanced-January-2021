function aggregateElements(nums){
    
    let sum1 = 0;
    for (let i = 0; i<nums.length; i++){
        sum1 += nums[i];
    }

    let sum2 = 0.00;
    for (let i = 0; i<nums.length; i++){
        sum2 += 1/nums[i];
    }

    let concat = '';
    for (let i = 0; i<nums.length; i++){
        concat += String(nums[i]);    
    }
    console.log(sum1);
    console.log(sum2);
    console.log(concat);

}
